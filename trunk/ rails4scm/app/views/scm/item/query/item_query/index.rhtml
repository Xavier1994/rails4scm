
<html>
  <head>  
        <meta http-equiv="Content-Type" content="text/html; charset=GB2312">  
	<meta http-equiv="keywords" content="keyword1,keyword2,keyword3">
	<meta http-equiv="description" content="This is my page">
        <%= stylesheet_link_tag "scaffold","style_table" %>
  </head>
  <body
      <% form_tag :action =>'item_query_controller' do %>
      <% end %>
      <%= tab_begin(@blockDefs,'cycle',nil) %> 
      <%= tabpage_begin('table_requests',true) %>
    
     <table cellpadding=0 cellspacing=0 border=0 style="width:100%;bordercolo:#878787 ;border-left-style: solid; border-left-width: 1px; border-right-style: solid; border-right-width: 1px">
       <tr>
         <td class="td_header_bak2" class="td_header_bak2">变更标识</td>
         <td class="td_header_bak3" class="td_header_bak3">配置项编号</td>
         <td class="td_header_bak3" class="td_header_bak3">配置项名称</td>
         <td class="td_header_bak3" class="td_header_bak3">事件标识</td>
         <td class="td_header_bak3" class="td_header_bak3">事件名称</td>
         <td class="td_header_bak3" class="td_header_bak3">相关事件标识</td>
         <td class="td_header_bak3" class="td_header_bak3">前版本</td>
         <td class="td_header_bak3" class="td_header_bak3">后版本</td>
         <td class="td_header_bak3" class="td_header_bak3">项目编号</td>
         <td class="td_header_bak3" class="td_header_bak3">当前状态</td>
      </tr>
      <% for conf in @conf%>
        <tr>
         <td class="td7"><%=conf.CONFIGURE_CHG_NO%></td>
         <td class="td7"><%=conf.CONFIGURE_CODE%></td>
         <td class="td7"><%=conf.CONFIGURE_NAME%></td>
         <td class="td7"><%=conf.EVENT_CODE%></td>
         <td class="td7"><%=conf.EVENT_NAME%></td>
         <td class="td7">&nbsp;<%=conf.RELA_EVENT_NAME%></td>
         <td class="td7"><%=conf.PRE_VERSION%></td>
         <td class="td7"><%=conf.AFT_VERSION%></td>
         <td class="td7"><%=conf.PROJECT_CODE%></td>
         <td class="td7"><%=conf.CURRENT_STATUS%></td>
        </tr>
      <% end%>
     </table>
        <%= will_paginate @conf %>

         <%= tabpage_end %>
         <%= tabpage_begin("cycle",false) %>
    	     this is page 2 content <br/><br/><br/><br/><br/>
         <%= tabpage_end %>
         <%= tab_end()%>
  </body>
</html>
